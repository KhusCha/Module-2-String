package coding.ninja;

import java.util.Scanner;

public class Test {		
	
	// Method 2
	public static void main(String[] args) {
		
		 String str ="pqrs";
		 
		for(int start =0;start<str.length();start++) {
			
			for(int end = start;end<str.length();end++) {
				
				System.out.println(str.substring(start, end+1));
			}
		} 
		
		
 }  
		
}	
